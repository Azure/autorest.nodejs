{
  "name": "@microsoft.azure/autorest.nodejs",
  "version": "2.2.1",
  "description": "The NodeJS extension for classic generators in AutoRest.",
  "scripts": {
    "autorest": "autorest",
    "start": "dotnet src/bin/netcoreapp2.0/autorest.nodejs.dll --server",
