@using System.Linq
@using AutoRest.NodeJS.vanilla.Templates
@inherits AutoRest.Core.Template<AutoRest.NodeJS.Model.CodeModelJs>
/*
@Header(" * ")
*/
@EmptyLine
import { ServiceClientOptions, RequestOptions, ServiceCallback, HttpOperationResponse } from 'ms-rest';
@foreach (var methodGroup in Model.MethodGroupModels)
{
@if (methodGroup.ContainsTimeSpan)
{
@:import * as moment from 'moment';
break;
}
}
@foreach (var methodGroup in Model.MethodGroupModels)
{
@if (methodGroup.ContainsStream)
{
@:import * as stream from 'stream';
break;
}
}
@if (Model.ModelTypes.Any() || Model.IsAzure) {
@:import * as models from '../models';
}
@EmptyLine
@foreach (var methodGroup in Model.MethodGroupModels) {
@EmptyLine
@:@(Include(new MethodGroupTemplateTS(), methodGroup))
}
