@using AutoRest.Core.Utilities;
@inherits AutoRest.Core.Template<AutoRest.NodeJS.Model.CodeModelJs>
{
  "name": "@(Model.PackageName)",
  "author": "Microsoft Corporation",
  "description": "@(Model.Name.ToPascalCase()) Library with typescript type definitions for node",
  "version": "@(Model.PackageVersion)",
  "dependencies": {
    @(Model.PackageDependenciesString())
  },
  "keywords": [
    "node",
    "azure"
  ],
  "license": "MIT",
  "main": "./lib/@(Model.Name.ToCamelCase()).js",
  "types": "./lib/@(Model.Name.ToCamelCase()).d.ts",
  "homepage": "@(Model.HomePageUrl)",
  "repository": {
    "type": "git",
    "url": "@(Model.RepositoryUrl)"
  },
  "bugs": {
    "url": "@(Model.BugsUrl)"
  }
}